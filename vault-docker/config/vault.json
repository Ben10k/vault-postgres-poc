{
  "listener": [
    {
      "tcp": {
        "address": "0.0.0.0:8200",
        "tls_cert_file": "/vault/config/server-cert.pem",
        "tls_key_file": "/vault/config/server-key.pem"
      }
    }
  ],
  "backend": {
    "file": {
      "path": "vault/file"
    }
  },
  "default_lease_ttl": "168h",
  "max_lease_ttl": "720h"
}
